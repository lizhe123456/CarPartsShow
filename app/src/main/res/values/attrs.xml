<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallSpinFadeLoader" value="22"/>
        </attr>
        <attr name="indicator_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="autoScrollHeight">
        <attr name="textSize" format="dimension"/>
        <attr name="padding" format="dimension"/>
        <attr name="animDuration" format="integer"/>
        <attr name="scrollDuration" format="integer"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SnapUpCountDownTimerView">
        <attr name="viewSize" format="integer"/>
    </declare-styleable>
</resources>